{
  "name" : "vs-point/grid-bundle",
  "type" : "symfony-bundle",
  "description" : "Ajax grid for Symfony3",
  "keywords" : [
    "datagrid",
    "grid"
  ],
  "homepage" : "https://github.com/vs-point/grid-bundle",
  "license" : "MIT",
  "authors" : [
    {
      "name" : "Pedro Teixeira",
      "email" : "hello@pedroteixeira.io",
      "homepage" : "https://pedroteixeira.io"
    },
    {
      "name" : "Tomáš Pokorný",
      "email" : "pokorny.tomas@vs-point.cz",
      "homepage" : "https://www.vs-point.cz"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/vs-point/grid-bundle"
    }
  ],
  "require" : {
    "php" : ">=7.0",
    "symfony/framework-bundle" : "~3",
    "symfony/intl": "~3",
    "doctrine/orm" : "~2.5",
    "twig/twig": "^2.0"
  },
  "require-dev" : {
    "squizlabs/php_codesniffer" : "~1"
  },
  "autoload" : {
    "psr-0" : {
      "PedroTeixeira\\Bundle\\GridBundle" : ""
    }
  },
  "config" : {
    "bin-dir" : "bin"
  },
  "target-dir" : "PedroTeixeira/Bundle/GridBundle"
}
